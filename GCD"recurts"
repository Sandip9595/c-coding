#include <stdio.h>

int main() 
{
 int x, y, gcd, min_n;
  printf("Enter two number: ");
 scanf("%d %d", &x, &y);
 if(x > y)
 {
  min_n = y;
 }
 else
 {
  min_n = x;
 }
for(int i = 2; i <= min_n; i++)
{
    if(x % i == 0 && y % i == 0)
  {
     gcd=i;
  printf("gcd of two numbers: %d\n",gcd);
  }
}
    return 0;
}

